            "id": "client-external",
            "zIndex": 1,
            "data": {
              "type": "tm.Actor",
              "name": "Client Externe",
              "description": "Utilisateur externe qui s'enregistre, se connecte, soumet des demandes et consulte le statut de ses factures",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "providesAuthentication": false,
              "threats": [
                {
                  "id": "threat-client-01",
                  "title": "Accès non autorisé à un compte client",
                  "status": "Open",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "Un attaquant pourrait accéder au compte d'un client légitime en utilisant des identifiants volés ou compromis",
                  "mitigation": "- Implémenter une authentification forte (mot de passe complexe, 2FA)\n- Limiter les tentatives de connexion\n- Utiliser des jetons de session sécurisés\n- Surveillance des connexions suspectes",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 1,
                  "score": "8.2"
                },
                {
                  "id": "threat-client-02",
                  "title": "Client nie avoir fait une demande",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Un client pourrait nier avoir soumis une demande de facture pro forma",
                  "mitigation": "- Enregistrer l'historique complet des actions du client (demandes soumises, dates, adresses IP)\n- Demander une confirmation par e-mail après la soumission d'une demande\n- Horodatage précis et traçabilité",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 2,
                  "score": "6.5"
                }
              ]
            }
