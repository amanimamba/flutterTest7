            "data": {
              "type": "tm.Actor",
              "name": "Employé/Administrateur",
              "description": "Utilisateur interne qui accède aux demandes, les gère, les valide, génère des factures pro forma et les envoie",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "providesAuthentication": false,
              "threats": [
                {
                  "id": "threat-employee-01",
                  "title": "Employé se faisant passer pour un autre",
                  "status": "Open",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "Un employé pourrait usurper l'identité d'un autre employé pour effectuer des actions non autorisées",
                  "mitigation": "- Utiliser une authentification et une autorisation basées sur les rôles (RBAC)\n- Enregistrer et auditer les actions sensibles de chaque employé\n- Sessions individuelles et traçables",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 3,
                  "score": "7.8"
                },
                {
                  "id": "threat-employee-02",
                  "title": "Employé nie avoir généré une facture",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Un employé pourrait nier avoir créé, modifié ou envoyé une facture pro forma",
                  "mitigation": "- Enregistrer l'ID de l'employé et la date/heure de chaque action (création, modification, envoi de facture)\n- Utiliser des signatures numériques pour les documents générés\n- Audit trail complet",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 4,
                  "score": "6.0"
                }
              ]
            }
