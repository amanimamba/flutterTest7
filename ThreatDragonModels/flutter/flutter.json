            "data": {
              "type": "tm.Store",
              "name": "Base de Données",
              "description": "Stockage des demandes de factures, données clients, informations employés et factures générées",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "hasOpenThreats": true,
              "isALog": false,
              "isEncrypted": true,
              "isSigned": false,
              "storesCredentials": true,
              "storesInventory": false,
              "threats": [
                {
                  "id": "threat-db-01",
                  "title": "Accès à des données client sensibles",
                  "status": "Open",
                  "severity": "Critical",
                  "type": "Information Disclosure",
                  "description": "Exposition non autorisée des données sensibles des clients (e-mails, adresses, informations financières)",
                  "mitigation": "- Stocker les données sensibles (e-mails, adresses) cryptées\n- Restreindre l'accès à la base de données via des pare-feux et des règles de sécurité\n- Authentification forte pour l'accès DB\n- Audit des accès aux données sensibles",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 9,
                  "score": "9.3"
                },
                {
                  "id": "threat-db-02",
                  "title": "Accès aux factures d'autres clients",
                  "status": "Open",
                  "severity": "High",
                  "type": "Information Disclosure",
                  "description": "Un client pourrait accéder aux factures et demandes d'autres clients",
                  "mitigation": "- Implémenter un contrôle d'accès strict (Authorization) pour garantir qu'un client ne peut voir que ses propres factures\n- Vérifier les ID de session et les permissions avant d'afficher un document\n- Isolation des données par client",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 10,
                  "score": "8.5"
                },
                {
                  "id": "threat-db-03",
                  "title": "Modification du statut d'une demande",
                  "status": "Open",
                  "severity": "High",
                  "type": "Tampering",
                  "description": "Modification non autorisée du statut des demandes de factures",
                  "mitigation": "- Le statut de la demande ne doit être modifiable que par un employé autorisé\n- Journaliser toutes les modifications de statut avec l'utilisateur et l'heure\n- Contrôles d'intégrité des données\n- Workflow d'approbation",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 11,
                  "score": "7.9"
                }
              ]
            }
